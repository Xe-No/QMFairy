Sub Hunt
Dim index
Call Lib.CC.GetSubAddress()
Call Lib.CC.GetWorldSize()

For index = 377 To 388
	Call Lib.CC.ReapBlock(index, "down")
Next

For index = 405 To 416
	Call Lib.CC.ReapBlock(index, "down")
Next

For index = 618 To 629
	If index = 621 Then 
		Call Lib.CC.ReapBlockEx(index, "down")
	Else
		Call Lib.CC.ReapBlock(index, "down")
	End If
Next

For index = 632 To 643
	If index = 632 Then 
		Call Lib.CC.ReapBlockEx(index, "down")
	Else
		Call Lib.CC.ReapBlock(index, "down")
	End If
Next






