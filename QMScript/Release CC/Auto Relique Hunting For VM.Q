[Script]

CaveSub = 121
CaveSup = 132
FossilSub = 149
FossilSup = 160
BreakPoint = False 
WorldName = "WorldT"

Call Lib.CC.Initailize()

dm_ret = dm.BindWindow(handle, "dx", "dx2", "dx", 0)
Delay 100
Dm.SetPath "C:\Documents and Settings\Administrator\My Documents\DmRes"'大漠全局路径设置


For Index = 1 To 50

Call TPMine(WorldName, Index)
Call JumpForward
Call WaitInnerWorld
Call Hunt
Call WaitOverWorld


Next


Sub TPMine(WorldName, MineIndex)
Dim MyString, MyArray
MinePos = Lib.文件.读取指定行文本内容("C:\Documents and Settings\Administrator\My Documents\DmRes\" & WorldName & ".txt", MineIndex)
If MinePos = "" Then 
EndScript
Else 
	MyString = Split(MinePos, ",", - 1 , 1)
	MineX = CSng(MyString(0))
	MineY = CSng(MyString(1))
	MineZ = CSng(MyString(2))
	返回值 = Lib.CC.TP(MineX, MineY, MineZ)
	Delay 1000
End If
	
	
End Sub


Sub JumpForward

Delay 200
dm.KeyDownChar "W"
Delay 16
dm.KeyDownChar "Space"
Delay 219
dm.KeyUpChar "Space"
Delay 31
dm.KeyUpChar "W"
Delay 200

End Sub


Sub Hunt

SubAddress = dm.ReadInt(handle, "[[<Cubic.exe>+23F6C]+5354]+760", 0)
Call Lib.CC.GetWorldSize()
dm_ret = dm.WriteFloat(handle, "[<Cubic.exe>+2A43F4]+83C", 0)
dm_ret = dm.WriteFloat(handle, "[<Cubic.exe>+2A43F4]+840", 89)

Dim MyArray(1), Addresses
MyArray(0) = CStr(Hex(SubAddress + 14))
MyArray(1) = CStr(Hex(SubAddress + SizeX*SizeY*SizeZ*14))
MyRange = Join(MyArray, "-")

TargetAdsArr = dm.FindIntEx(handle, MyRange, CaveSub, CaveSup, 2, 14, 0, 0) & "|" & dm.FindIntEx(handle, MyRange, FossilSub, FossilSup, 2, 14, 0, 0)

Addresses = Split(TargetAdsArr, "|")

Lock = 0
BlockCount = 0
For Each I In Addresses
	BlockID = AdrToPos(I,0)
	BlockType = AdrToPos(I, 1) Mod 16
	If BlockType = 1 And BlockID <> 134 Then 
		X = AdrToPos(I, 2)
		Y = AdrToPos(I, 3)
		Z = AdrToPos(I, 4)
		Lock = 1
		BlockCount = BlockCount + 1
		If BlockID > 120 And BlockID < 133 Then 
			Block = "CaveArt!!!"
			Else 
			Block = "Fossil!!!"
		End If
		Goto TP
	End If
Next


Goto SkipHunting
Rem TP

For i = Y To SizeY - 1
For j = 40 To Z - 1
	Call Lib.CC.WriteBlock(X, i ,j, 0)
Next
Next

Call Lib.CC.TP(X, Y -.2, Z - 1)
Delay 500
Call Plugin.Bkgnd.LeftClick(handle, 640, 480)
Delay 2000
dm.KeyPressChar "Space"
Delay 219
Dk = 50
	While Lib.CC.ReadBlock(X , Y -1 , Dk) = 0 And Dk - SizeZ  < 0
		Dk = Dk + 1
		Delay 100
	Wend
Delay 200
Call Lib.CC.TP(X, Y - 1, Dk - 1)

If BreakPoint Then
	dm.WaitKey 115, 0
	Goto SkipHunting
End If
Delay 2000

Rem SkipHunting
Call Plugin.Bkgnd.LeftClick(handle, 1256, 22)
Delay 100
Call Plugin.Bkgnd.LeftClick(handle, 560, 536)
Delay 100
End Sub


Sub WaitInnerWorld
intX = 0	
Do
dm_ret = dm.FindPic(0, 0, 2000, 2000, "ExitWorld.bmp", "000000", 0.9, 2, intX, intY)
Delay 200
Loop While intX < 0

End Sub


Sub WaitOverWorld

intX = 0
Do
dm_ret = dm.FindPic(0, 0, 2000, 2000, "SkyMap.bmp", "000000", 0.9, 2, intX, intY)
Delay 200
Loop While intX < 0

End Sub


Function AdrToPos(adr, off)
	Dim Arr(1)
	Arr(0) = adr
	Arr(1) = CStr(off)
	PStr = Join(Arr,"+")
	AdrToPos = dm.ReadInt(handle,PStr,2)
End Function
